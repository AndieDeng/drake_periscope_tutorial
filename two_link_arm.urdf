<?xml version="1.0" ?>
<robot name="two_link_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">
<link name="base"/>

<!-- joint between base and link_0  -->
<joint name="base_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
	<parent link="base"/>
	<child link="link_0"/>
</joint>

<link name="link_0">
	<visual>
		<origin rpy="0 0 0" xyz="0 0 0.05"/>
		<geometry>
			<sphere radius="0.05"/>
		</geometry>
	</visual>
</link>

<!-- joint between link_0 and link_1 -->
<joint name="joint_1" type="revolute">
	<parent link="link_0"/>
	<child link="link_1"/>
	<origin rpy="0 0 0" xyz="0 0 0.05"/>
	<axis xyz="1 0 0"/>
<limit effort="100" lower="-2.96705972839" upper="2.96705972839" velocity="10"/>
</joint>

<link name="link_1">
	<inertial>
		  <origin xyz="0 0.5 0" rpy="0 0 0" />
		  <mass value="1" />
		  <inertia ixx="0.0833" ixy="0" ixz="0" iyy="0" iyz="0" izz="0.0833" />
    </inertial>

	<visual>
		<origin xyz="0 0.5 0" rpy="1.5708 0 0"/>
		<geometry>
			<cylinder length="0.9" radius="0.05"/>
		</geometry>
	</visual>

	<visual>
		<origin xyz="0 1.0 0" rpy="0 0 0"/>
		<geometry>
			<sphere radius="0.05"/>
		</geometry>
	</visual>
</link>

<!-- joint between link_1 and link_2 -->
<joint name="joint_2" type="revolute">
	<parent link="link_1"/>
	<child link="link_2"/>
	<origin rpy="0 0 0" xyz="0 1 0"/>
	<axis xyz="1 0 0"/>
<limit effort="100" lower="-2.96705972839" upper="2.96705972839" velocity="10"/>
</joint>

<link name="link_2">
	<inertial>
		  <origin xyz="0 0.5 0" rpy="0 0 0" />
		  <mass value="1" />
		  <inertia ixx="0.0833" ixy="0" ixz="0" iyy="0" iyz="0" izz="0.0833" />
    </inertial>

	<visual>
		<origin xyz="0 0.5 0" rpy="1.5708 0 0"/>
		<geometry>
			<cylinder length="0.9" radius="0.05"/>
		</geometry>
	</visual>

	<visual>
		<origin xyz="0 1.0 0" rpy="0 0 0"/>
		<geometry>
			<sphere radius="0.05"/>
		</geometry>
	</visual>

	<collision>
		<origin xyz="0 1.0 0" rpy="0 0 0"/>
		<geometry>
			<sphere radius="0.05"/>
		</geometry>		
	</collision>
</link>

<!-- joint between link_2 and end effector link -->
<joint name="joint_ee" type="fixed">
	<parent link="link_2"/>
	<child link="link_ee"/>
	<origin rpy="0 0 0" xyz="0 1 0"/>
</joint>

<link name="link_ee"/>

<transmission name="tran_1">
	<robotNamespace>/two_link_arm</robotNamespace>
	<type>transmission_interface/SimpleTransmission</type>
	<joint name="joint_1">
	  <hardwareInterface>PositionJointInterface</hardwareInterface>
	</joint>
	<actuator name="motor_1">
	  <hardwareInterface>PositionJointInterface</hardwareInterface>
	  <mechanicalReduction>1</mechanicalReduction>
	</actuator>
</transmission>

<transmission name="tran_2">
	<robotNamespace>/two_link_arm</robotNamespace>
	<type>transmission_interface/SimpleTransmission</type>
	<joint name="joint_2">
	  <hardwareInterface>PositionJointInterface</hardwareInterface>
	</joint>
	<actuator name="motor_2">
	  <hardwareInterface>PositionJointInterface</hardwareInterface>
	  <mechanicalReduction>1</mechanicalReduction>
	</actuator>
</transmission>

</robot>